<a href="/form">Add New</a>
<table class="table table-striped">
    <thead>
        <tr>
            <th>Name</th>
            <th>Roll No</th>
            <th>Class</th>
            <th></th>
        </tr>
    </thead>
    <tbody>
        {{#each list}}
            <tr>
                <td>{{this.name}}</td>
                <td>{{this.rollNumber}}</td>
                <td>{{this.class}}</td>
                <td>
                    <a href="/form/{{this._id}}"><i class="fas fa-pencil-alt" aria-hidden="true"></i></a>
                </td>
                <td>
                    <a href="/form/delete/{{this._id}}" onclick="return confirm('Are You Sure You want to delete this record?');"><i class="fa fa-trash" aria-hidden="true"></i></a>
                </td>
            </tr>
        {{/each}}
    </tbody>
</table>